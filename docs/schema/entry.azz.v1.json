{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://mesolex.nyc3.digitaloceanspaces.com/schema/entry.azz.v1.json",
  "title": "SNP Nahuat lexical entry data",
  "description": "Lexical entry data for SNP Nahuat",
  "type": "object",
  "properties": {
    "headword": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "meta": {
      "type": "object",
      "properties": {
        "date": {
          "type": "string"
        },
        "variant_data": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "roots": {
      "type": "object",
      "properties": {
        "simple": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "compound": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "citation_forms": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "variant_forms": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "glosses": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "note_type": {
            "type": "string"
          },
          "text": {
            "type": "string"
          }
        }
      }
    },
    "senses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "geo": {
            "type": "string"
          },
          "sense": {
            "type": "string"
          },
          "ostentives": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "examples": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "geo": {
                  "type": "string"
                },
                "original": {
                  "type": "object",
                  "properties": {
                    "text": {
                      "type": "string"
                    },
                    "language": {
                      "type": "string"
                    }
                  }
                },
                "translation": {
                  "text": {
                    "type": "string"
                  },
                  "language": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "grammar_groups": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "part_of_speech": {
            "type": "string"
          },
          "inflectional_type": {
            "type": "string"
          },
          "other_data": {
            "type": "object",
            "properties": {
              "plural": {
                "type": "string"
              },
              "diag": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "non_native_etymologies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    }
  }
}