{
  "presets": [
    "airbnb",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"]
        },
        "useBuiltIns": "entry",
        "corejs": "2.x"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": false }]]
}
